def countAndSay(n: int) -> str:
    if n == 1:
        return "1"

    def get_next(s: str) -> str:
        result = []
        i = 0
        while i < len(s):
            count = 1
            while i + 1 < len(s) and s[i] == s[i + 1]:
                i += 1
                count += 1
            result.append(f"{count}{s[i]}")
            i += 1
        return ''.join(result)

    result = "1"
    for _ in range(n - 1):
        result = get_next(result)
    
    return result


print(countAndSay(4)) 
